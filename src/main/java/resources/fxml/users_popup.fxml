<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" onMouseDragged="#dragged"
	onMousePressed="#pressed" prefHeight="630.0" prefWidth="300.0"
	stylesheets="@../styles/popup_style.css"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="alfatec.controller.user.UsersPopupController">
	<children>
		<VBox id="popup" alignment="TOP_CENTER"
			maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
			minWidth="-Infinity" prefHeight="630.0" prefWidth="300.0">
			<children>
				<HBox alignment="TOP_RIGHT" maxHeight="-Infinity"
					maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
					prefHeight="25.0" prefWidth="300.0">
					<children>
						<Button fx:id="closeButton" maxHeight="-Infinity"
							maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
							mnemonicParsing="false" onAction="#close" prefHeight="25.0"
							prefWidth="25.0">
							<graphic>
								<ImageView fitHeight="25.0" fitWidth="25.0"
									pickOnBounds="true" preserveRatio="true">
									<image>
										<Image url="@../images/quit_login_icon.png" />
									</image>
								</ImageView>
							</graphic>
							<HBox.margin>
								<Insets right="5.0" top="3.0" />
							</HBox.margin>
						</Button>
					</children>
				</HBox>
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="First name:" />
				<TextField id="text" fx:id="firstNameTextField"
					layoutX="15.0" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0"
					prefWidth="200.0">
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</TextField>
				<Label id="errorText" fx:id="firstNameErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="Last name:" />
				<TextField id="text" fx:id="lastNameTextField"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0">
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</TextField>
				<Label id="errorText" fx:id="lastNameErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="E-mail:" />
				<TextField id="text" fx:id="emailTextField"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0">
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</TextField>
				<Label id="errorText" fx:id="emailErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="Mobile" />
				<TextField id="text" fx:id="mobileTextField"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0">
					<VBox.margin>
						<Insets />
					</VBox.margin>
				</TextField>
				<Label id="errorText" fx:id="MobileErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="Role:" />
				<ComboBox id="comboBoxText" fx:id="roleComboBox"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0"
					promptText="Please select" />
				<Label id="errorText" fx:id="roleErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="Password:" />
				<PasswordField id="text" fx:id="passwordField"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" />
				<Label id="errorText" fx:id="passwordErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<Label id="label" maxHeight="-Infinity" maxWidth="-Infinity"
					minHeight="-Infinity" minWidth="-Infinity" prefHeight="25.0"
					prefWidth="200.0" text="Confirm password:" />
				<PasswordField id="text" fx:id="confirmPasswordField"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="30.0" prefWidth="200.0" />
				<Label id="errorText" fx:id="confirmPasswordErrorLabel"
					maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
					minWidth="-Infinity" prefHeight="20.0" prefWidth="200.0" />
				<JFXButton id="button" fx:id="saveUserButton"
					buttonType="RAISED" onAction="#saveUser" prefHeight="40.0"
					prefWidth="100.0" text="Save">
					<VBox.margin>
						<Insets top="10.0" />
					</VBox.margin>
				</JFXButton>
			</children>
		</VBox>
	</children>
</AnchorPane>
